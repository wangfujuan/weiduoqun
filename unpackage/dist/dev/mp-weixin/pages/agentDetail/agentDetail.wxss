@import "../../common/vendor.wxss";

.content {
  background-color: #F4F6FA;
}
.openlist {
  padding: 0 20rpx;
  background-color: #fff;
  margin: 20rpx 0;
}
.open-item {
  padding: 24rpx 0;
  color: #505050;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
}
.open-item-lf {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 200rpx;
  flex: 0 0 200rpx;
  text-align: right;
}
.open-item-rt {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  padding-left: 20rpx;
}
.fontred {
  color: #e86a38;
}
.fontyellow {
  color: #00b8a7;
}
.expiretime {
  color: #000000;
  font-weight: 500;
}
.larfont {
  font-size: 28rpx;
  font-weight: 500;
}
.open-tips {
  background-color: #fff;
  padding: 40rpx 60rpx 30rpx;
}
.open-tips-tl {
  padding-bottom: 20rpx;
  text-align: center;
  color: #b2b2b2;
}
.open-tips-line {
  font-size: 26rpx;
  color: #999;
  margin-bottom: 14rpx;
}