.gray {
  color: #585858;
}
.logo {
  text-align: center;
  margin: 100rpx 0;
}
.input-group {
  position: relative;
  padding: 0 40rpx;
}
.input-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  position: relative;
}
.input-row ._input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 50rpx;
  padding: 30rpx 0;
}
.btn-row {
  margin-top: 80rpx;
  padding: 0 40rpx;
  margin-bottom: 80rpx;
}
uni-button:after {
  border-width: 0;
}
.loginbtn {
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border-radius: 90rpx;
  font-size: 28rpx;
}
.action-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  padding: 0 60rpx;
  font-size: 26rpx;
}
.oauth-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding-top: 120rpx;
}
.oauth-image {
  width: 100rpx;
  height: 100rpx;
  margin: 0 40rpx;
}
.oauth-image image {
  width: 100rpx;
  height: 100rpx;
}
.weixinlogin {
  margin-bottom: 80rpx;
  color: #505050;
}
.weixin-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  margin-top: 30rpx;
  color: #999;
}
.weixin-info navigator {
  color: #44B549;
}